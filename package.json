{
  "name": "theoryjs",
  "version": "0.0.1",
  "description": "A simple music theory library for javascript",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha --require babel-core/register",
    "build": "babel src -d lib",
    "watch": "concurrently --kill-others \"babel src -d lib --watch\" \"esw -w src --color\"",
    "lint": "esw -w --color src"
  },
  "author": "Corey White",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-lodash": "^3.2.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.4.1",
    "concurrently": "^2.2.0",
    "eslint": "^2.12.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-watch": "^2.1.13",
    "mocha": "^2.3.3",
    "npm-run-parallel": "^0.4.3"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "lodash-es": "^4.13.1"
  }
}
